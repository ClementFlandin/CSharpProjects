
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListeClients</title>
</head>
<body>
    <table>
        <tr>
            <th>Nom</th>
            <th>Age</th>
        </tr>
        @foreach (WebApplication1.Models.Client client in ViewBag.Clients as List<WebApplication1.Models.Client>)
        {
            <tr>
                <th>@client.Nom</th>
                <th>@client.Age</th>
            </tr>
        }
    </table>
</body>
</html>
